{
  "roles": [
    {
      "name": "Tasks administrator",
      "description": "Perform any available operation on any Tasks resource.",
      "system": true,
      "platform_default": false,
      "admin_default": true,
      "version": 5,
      "access": [
        {
          "permission": "tasks:*:*"
        },
        {
          "permission": "playbook-dispatcher:tasks_run:read"
        },
        {
          "permission": "playbook-dispatcher:run:read",
          "resourceDefinitions": [
            {
              "attributeFilter": {
                "key": "service",
                "operation": "equal",
                "value": "tasks"
              }
            }
          ]
        }
      ]
    }
  ]
}
