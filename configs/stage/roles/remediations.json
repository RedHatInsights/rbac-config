      "version": 6,
      "access": [
        {
          "permission": "remediations:remediation:read"
        },
        {
          "permission": "remediations:remediation:write"
        },
        {
          "permission": "playbook-dispatcher:remediations_run:read"
        }
      ]
    }
  ]
